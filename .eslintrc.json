{
    "env": {
        "browser": true,
        "es2021": true,
        "react": true,
        "javascript": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "javascript",
        "html"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ]
    }
}